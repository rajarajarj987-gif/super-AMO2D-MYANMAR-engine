<script>
    const API_KEY = "2d33e7fd9cmshed62a232961d59cp1e7e91jsn5690f63247a7";
    const API_HOST = "myanmar-all-in-one-2d-results.p.rapidapi.com";
    
    let lastData = null;
    let drawTimes = [
        { time: "12:01", elementId: "confirm-12", type: "morning" },
        { time: "16:31", elementId: "confirm-16", type: "evening" }
    ];

    function openPage(pageId, btn) {
        document.querySelectorAll('.page').forEach(p => p.classList.remove('active'));
        document.querySelectorAll('.proton-tab').forEach(b => b.classList.remove('active'));
        document.getElementById(pageId).classList.add('active');
        btn.classList.add('active');
        
        // Add proton effect
        btn.style.transform = "translateY(-3px)";
        setTimeout(() => {
            if(btn.classList.contains('active')) {
                btn.style.transform = "translateY(-2px)";
            } else {
                btn.style.transform = "translateY(0)";
            }
        }, 300);
    }

    function updateClock() {
        const now = new Date();
        const hours = now.getHours().toString().padStart(2, '0');
        const minutes = now.getMinutes().toString().padStart(2, '0');
        const seconds = now.getSeconds().toString().padStart(2, '0');
        const currentTime = `${hours}:${minutes}:${seconds}`;
        
        // Add proton pulsing effect to clock
        const clockEl = document.getElementById('live-clock');
        clockEl.textContent = `ðŸ•’ ${currentTime}`;
        
        // Change color based on time
        if (seconds % 2 === 0) {
            clockEl.style.color = "#00ff00";
            clockEl.style.textShadow = "0 0 10px #00ff00";
        } else {
            clockEl.style.color = "#666";
            clockEl.style.textShadow = "none";
        }
        
        // Check if it's draw time
        const currentHM = `${hours}:${minutes}`;
        
        drawTimes.forEach(draw => {
            if (currentHM === draw.time) {
                // Proton animation for draw time
                const drawEl = document.getElementById(draw.elementId);
                if(drawEl) {
                    drawEl.style.animation = "none";
                    setTimeout(() => {
                        drawEl.style.animation = "protonPulse 0.5s ease";
                    }, 10);
                }
                
                // Fetch new data
                fetchAllData();
                
                // Show notification
                showProtonNotification(`âš¡ ${draw.type.toUpperCase()} DRAW TIME!`);
            }
        });
    }

    function showProtonNotification(message) {
        // Create proton notification
        const notification = document.createElement('div');
        notification.textContent = message;
        notification.style.cssText = `
            position: fixed;
            top: 20px;
            left: 50%;
            transform: translateX(-50%);
            background: linear-gradient(145deg, #000, #111);
            color: var(--gold);
            padding: 15px 25px;
            border-radius: 15px;
            border: 2px solid var(--gold);
            font-weight: 900;
            z-index: 1000;
            box-shadow: 0 10px 30px rgba(212, 175, 55, 0.4);
            animation: protonSlideIn 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
        `;
        
        document.body.appendChild(notification);
        
        setTimeout(() => {
            notification.style.animation = "protonSlideOut 0.5s ease forwards";
            setTimeout(() => notification.remove(), 500);
        }, 3000);
    }

    async function fetchAllData() {
        try {
            // Proton loading effect
            const refreshBtn = document.querySelector('.refresh-btn');
            refreshBtn.innerHTML = "ðŸ”„ LOADING...";
            refreshBtn.style.background = "linear-gradient(145deg, #333, #000)";
            
            const liveRes = await fetch(`https://${API_HOST}/api/v1/live`, {
                headers: {
                    'x-rapidapi-key': API_KEY,
                    'x-rapidapi-host': API_HOST
                }
            });
            
            const liveData = await liveRes.json();
            
            if (liveData.data) {
                lastData = liveData.data;
                updateLiveDisplay(lastData);
                updateHistory(lastData);
                
                // Proton success effect
                refreshBtn.innerHTML = "âœ… DATA UPDATED";
                refreshBtn.style.background = "linear-gradient(145deg, #2ecc71, #27ae60)";
                setTimeout(() => {
                    refreshBtn.innerHTML = "âš¡ REFRESH ALL DATA";
                    refreshBtn.style.background = "linear-gradient(145deg, var(--gold), #b8860b)";
                }, 2000);
                
                // Save to cache
                localStorage.setItem('cached_data', JSON.stringify(liveData.data));
                localStorage.setItem('cached_time', new Date().toISOString());
                
                // Proton update animation
                protonUpdateAnimation();
            }
        } catch (error) {
            console.log('Using cached data...');
            
            // Proton error effect
            refreshBtn.innerHTML = "âŒ OFFLINE MODE";
            refreshBtn.style.background = "linear-gradient(145deg, #ff4757, #c23616)";
            setTimeout(() => {
                refreshBtn.innerHTML = "âš¡ REFRESH ALL DATA";
                refreshBtn.style.background = "linear-gradient(145deg, var(--gold), #b8860b)";
            }, 2000);
            
            const cachedData = localStorage.getItem('cached_data');
            if (cachedData) {
                lastData = JSON.parse(cachedData);
                updateLiveDisplay(lastData);
                document.getElementById('live-clock').textContent = "ðŸ“¡ OFFLINE (Cached Data)";
                document.getElementById('live-clock').style.color = "#ff4757";
            }
        }
    }

    function protonUpdateAnimation() {
        // Add proton effect to all number elements
        const numberElements = document.querySelectorAll('.main-live-number, .confirm-value, .pair-value, .realtime-value');
        numberElements.forEach(el => {
            el.style.transform = "scale(1.1)";
            el.style.textShadow = "0 0 30px currentColor";
            setTimeout(() => {
                el.style.transform = "scale(1)";
                el.style.textShadow = "0 0 15px rgba(255,255,255,0.1)";
            }, 300);
        });
    }

    function updateLiveDisplay(data) {
        // Proton number animation
        const updateWithProton = (elementId, value) => {
            const element = document.getElementById(elementId);
            if(element) {
                element.style.transform = "scale(1.2)";
                element.style.opacity = "0.7";
                setTimeout(() => {
                    element.textContent = value || "--";
                    element.style.transform = "scale(1)";
                    element.style.opacity = "1";
                }, 150);
            }
        };
        
        // Live 2D á€‚á€á€”á€ºá€¸
        updateWithProton('live-2d', data.live);
        updateWithProton('live-12', data.live);
        updateWithProton('live-16', data.live);
        updateWithProton('live-3d', data.three_d);
        updateWithProton('tw-2d', data.tw_result);
        updateWithProton('confirm-12', data.mResult);
        updateWithProton('confirm-16', data.eResult);
        updateWithProton('mod-09', data.mModern);
        updateWithProton('mod-14', data.eModern);
        updateWithProton('int-12', data.mInternet);
        updateWithProton('int-16', data.eInternet);
    }

    function updateHistory(data) {
        const historyBody = document.getElementById('history-body');
        
        const today = new Date();
        const dateStr = today.toLocaleDateString('my-MM', {
            day: '2-digit',
            month: 'short',
            year: 'numeric'
        });
        
        // Proton table animation
        historyBody.style.opacity = "0.5";
        historyBody.style.transform = "translateY(10px)";
        
        setTimeout(() => {
            let html = `
                <tr>
                    <td class="date-cell">${dateStr}</td>
                    <td><span style="color: var(--modern); font-weight: 900;">12:01</span></td>
                    <td style="color: var(--gold); font-size: 20px; font-weight: 900; text-shadow: 0 0 10px rgba(212,175,55,0.3);">${data.mResult || "--"}</td>
                </tr>
                <tr>
                    <td class="date-cell">${dateStr}</td>
                    <td><span style="color: var(--internet); font-weight: 900;">16:30</span></td>
                    <td style="color: var(--gold); font-size: 20px; font-weight: 900; text-shadow: 0 0 10px rgba(212,175,55,0.3);">${data.eResult || "--"}</td>
                </tr>
            `;
            
            historyBody.innerHTML = html;
            historyBody.style.opacity = "1";
            historyBody.style.transform = "translateY(0)";
        }, 200);
    }

    // Initialize with PROTONS POWER
    window.onload = function() {
        // Add CSS animations
        const style = document.createElement('style');
        style.textContent = `
            @keyframes protonSlideIn {
                from { top: -100px; opacity: 0; }
                to { top: 20px; opacity: 1; }
            }
            @keyframes protonSlideOut {
                from { top: 20px; opacity: 1; }
                to { top: -100px; opacity: 0; }
            }
            @keyframes protonPulse {
                0%, 100% { transform: scale(1); text-shadow: 0 0 15px currentColor; }
                50% { transform: scale(1.1); text-shadow: 0 0 30px currentColor; }
            }
        `;
        document.head.appendChild(style);
        
        // Initial fetch
        fetchAllData();
        
        // Proton clock
        setInterval(updateClock, 1000);
        updateClock();
        
        // Auto refresh every 30 seconds
        setInterval(fetchAllData, 30000);
        
        // Initial proton animation
        setTimeout(() => {
            protonUpdateAnimation();
        }, 1000);
    };
</script>
</body>
</html>
